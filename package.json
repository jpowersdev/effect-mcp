{
  "name": "@jpowersdev/effect-mcp",
  "version": "0.1.0",
  "description": "Effect implementation of Model Context Protocol (MCP) transports",
  "keywords": [
    "typescript",
    "effect-ts",
    "mcp",
    "model-context-protocol",
    "ai"
  ],
  "homepage": "https://github.com/jpowersdev/effect-mcp",
  "bugs": {
    "url": "https://github.com/jpowersdev/effect-mcp/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpowersdev/effect-mcp"
  },
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && pnpm build-utils pack-v2",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "check": "tsc -b tsconfig.json",
    "circular": "node scripts/circular.mjs",
    "codegen": "scripts/generate.sh",
    "clean": "node scripts/clean.mjs",
    "coverage": "vitest run --coverage",
    "dev": "nodemon",
    "test": "vitest",
    "inspect": "pnpm dlx @modelcontextprotocol/inspector",
    "changeset-version": "changeset version && node scripts/version.mjs",
    "changeset-publish": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@effect/ai": "^0.13.2",
    "@effect/experimental": "^0.44.2",
    "@effect/opentelemetry": "^0.45.2",
    "@effect/platform": "^0.80.2",
    "@effect/sql": "^0.33.2",
    "@effect/sql-sqlite-node": "^0.34.2",
    "@opentelemetry/exporter-logs-otlp-http": "^0.57.2",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.57.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.2",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-logs": "^0.57.2",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "effect": "^3.14.2",
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@edge-runtime/vm": "^5.0.0",
    "@effect/build-utils": "^0.7.9",
    "@effect/dtslint": "^0.1.2",
    "@effect/eslint-plugin": "^0.3.0",
    "@effect/language-service": "^0.4.0",
    "@effect/platform-node": "^0.76.5",
    "@effect/vitest": "^0.20.2",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@jpowersdev/jsonschema-gen": "^0.0.3",
    "@modelcontextprotocol/inspector": "^0.7.0",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/expect": "^3.0.9",
    "@vitest/web-worker": "^3.0.9",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.2.7",
    "eslint-plugin-import-x": "^4.9.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "fast-check": "^4.0.1",
    "glob": "^11.0.1",
    "madge": "^8.0.0",
    "nodemon": "^3.1.9",
    "playwright": "^1.51.1",
    "prettier": "^3.5.3",
    "repomix": "^0.3.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af"
}
